<!-- DataTales Example -->
<div class="card shadow mb-4">
  <div class="card-header py-3">
    <!-- Button trigger modal -->
    <!-- <button type="button" class="btn btn-dark" data-toggle="modal" data-target="#add-product">
      <i class="fas fa-plus"></i>
    </button> -->
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th>No</th>
            <th>No Member</th>
            <th>Nama</th>
            <th>NIK</th>
            <th>Instagram</th> 
            <th>No HP</th>
            <th>Status</th>
          </tr>
        </thead>
        <tbody>
          <% for(let i = 0; i < member.length; i++) { %>
          <tr>
            <td><%= i + 1 %></td>
            <td><%= member[i].no_member %></td>
            <td><%= member[i].name %></td>
            <td><%= member[i].identity %></td>
            <td><%= member[i].username_ig %></td>
            <td><%= member[i].no_hp %></td>
            <td><%= member[i].status %></td>
          </tr>
          <% }%>
        </tbody>
      </table>
    </div>
  </div>
</div>
