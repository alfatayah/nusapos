<div class="tab-pane fade show active" id="show-transaction" role="tabpanel" aria-labelledby="home-tab">
  <!-- DataTales Example -->
  <div class="card shadow mb-4 mt-2">
    <div class="card-body">
      <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
          <thead>
            <tr >
              <th>No</th>
              <th>No Member</th> 
              <th>Member</th> 
              <th>Date</th> 
              <th>Jaminan</th>
              <th>Total</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <% for(let i = 0; i < trans.length; i++) { %>
            <tr  >
              <td><%= i + 1 %></td>
              <td><%= trans[i].member_Id.no_member %></td>
              <td><%= trans[i].member_Id.name %></td>
              <td><%= trans[i].date_transaction %></td>
              <td><%= trans[i].jaminan %></td>
              <td><%= trans[i].total %></td>
              <td>
                <a href="/admin/transaction/<%= trans[i].transdetail_id %> " class="btn btn-success btn-circle btn-sm button-update">
                  <i class="fas fa-eye"></i></a>
                <a href="/admin/transaction/print/<%= trans[i].transdetail_id %>  "
                  class="btn btn-success btn-circle btn-sm button-update">
                  <i class="fas fa-print"></i></a>
              </td>
              </tr>
            <% }%>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>

